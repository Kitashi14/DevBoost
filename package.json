{
  "name": "devboost",
  "displayName": "DevBoost",
  "description": "DevBoost is a Visual Studio Code extension built to supercharge developer productivity through AI-driven automation and workflow optimization. Features include SmartCmd for one-click automation buttons, Prompt Enhancer for improving AI interactions (like Webex rewrite message), and intelligent activity logging for better AI suggestions.",
  "version": "0.2.0",
  "publisher": "devboost",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "devboost-sidebar",
          "title": "DevBoost",
          "icon": "$(rocket)"
        }
      ]
    },
    "views": {
      "devboost-sidebar": [
        {
          "id": "devboost.smartCmdView",
          "name": "SmartCmd",
          "icon": "$(terminal)"
        },
        {
          "id": "devboost.promptEnhancerView",
          "name": "Prompt Enhancer",
          "icon": "$(sparkle)"
        }
      ]
    },
    "commands": [
      {
        "command": "DevBoost.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "devboost.smartCmdCreateButtons",
        "title": "SmartCmd: Create AI Suggested Buttons for this workspace",
        "icon": "$(sparkle)"
      },
      {
        "command": "devboost.smartCmdCreateCustomButton",
        "title": "SmartCmd: Create Custom Button",
        "icon": "$(add)"
      },
      {
        "command": "devboost.executeButton",
        "title": "Execute Button",
        "icon": "$(play)"
      },
      {
        "command": "devboost.deleteButton",
        "title": "Delete Button",
        "icon": "$(trash)"
      },
      {
        "command": "devboost.editButton",
        "title": "View/Edit Button Details",
        "icon": "$(edit)"
      },
      {
        "command": "devboost.refreshButtons",
        "title": "Refresh Buttons",
        "icon": "$(refresh)"
      },
      {
        "command": "devboost.openButtonsFile",
        "title": "Open Buttons File",
        "icon": "$(file-code)"
      },
      {
        "command": "devboost.addToGlobal",
        "title": "Add to Global Buttons",
        "icon": "$(globe)"
      },
      {
        "command": "devboost.showPromptEnhancer",
        "title": "DevBoost: Show Prompt Enhancer",
        "icon": "$(sparkle)"
      },
      {
        "command": "devboost.quickEnhance",
        "title": "DevBoost: Quick Enhance from Clipboard",
        "icon": "$(zap)"
      },
      {
        "command": "devboost.enhancePromptFromInput",
        "title": "DevBoost: Enhance Prompt from Input",
        "icon": "$(sparkle)"
      },
      {
        "command": "devboost.openScriptFile",
        "title": "Open Script File",
        "icon": "$(file-code)"
      },
      {
        "command": "devboost.changeSmartCmdAIModel",
        "title": "Change AI Model for SmartCmd",
        "icon": "$(settings-gear)"
      },
      {
        "command": "devboost.changePromptEnhancerAIModel",
        "title": "Change AI Model for Prompt Enhancer",
        "icon": "$(settings-gear)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "devboost.showPromptEnhancer"
        }
      ],
      "view/title": [
        {
          "command": "devboost.smartCmdCreateButtons",
          "when": "view == devboost.smartCmdView",
          "group": "navigation@1"
        },
        {
          "command": "devboost.smartCmdCreateCustomButton",
          "when": "view == devboost.smartCmdView",
          "group": "navigation@2"
        },
        {
          "command": "devboost.refreshButtons",
          "when": "view == devboost.smartCmdView",
          "group": "navigation@3"
        },
        {
          "command": "devboost.changeSmartCmdAIModel",
          "when": "view == devboost.smartCmdView",
          "group": "navigation@4"
        },
        {
          "command": "devboost.changePromptEnhancerAIModel",
          "when": "view == devboost.promptEnhancerView",
          "group": "navigation@1"
        }
      ],
      "view/item/context": [
        {
          "command": "devboost.openButtonsFile",
          "when": "view == devboost.smartCmdView && viewItem == section",
          "group": "inline@1"
        },
        {
          "command": "devboost.smartCmdCreateCustomButton",
          "when": "view == devboost.smartCmdView && viewItem == section",
          "group": "inline@2"
        },
        {
          "command": "devboost.editButton",
          "when": "view == devboost.smartCmdView && viewItem =~ /Button$/",
          "group": "1_modify@1"
        },
        {
          "command": "devboost.addToGlobal",
          "when": "view == devboost.smartCmdView && viewItem == workspaceButton",
          "group": "1_modify@3"
        },
        {
          "command": "devboost.openScriptFile",
          "when": "view == devboost.smartCmdView && viewItem =~ /ScriptButton$/",
          "group": "1_modify@2"
        },
        {
          "command": "devboost.addToGlobal",
          "when": "view == devboost.smartCmdView && (viewItem == workspaceButton || viewItem == workspaceScriptButton)",
          "group": "1_modify@3"
        },
        {
          "command": "devboost.deleteButton",
          "when": "view == devboost.smartCmdView && viewItem =~ /Button$/",
          "group": "1_modify@4"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.2",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  }
}
